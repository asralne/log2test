{
    "name": "ousamabenyounes/log2test",
    "description": "Transorm your production logs to simple selenium/phpunit tests...",
    "license": "Apache-2.0",
    "require": {
        "kassner/log-parser": "~1.0",
        "twig/twig": "~1.0",
        "symfony/yaml": "^3.1",
        "incenteev/composer-parameter-handler": "~2.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ]
    },
    "extra": {
        "incenteev-parameters": {
            "file": "config/parameters.yml"
        }
    },

    "autoload": {
        "psr-4" : {
            "Log2Test\\" : "lib/"
        }
    }
}
